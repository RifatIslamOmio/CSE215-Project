<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StartPoint">
   <children>
      <ImageView fx:id="background" fitHeight="373.0" fitWidth="563.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images%20(2).png" />
         </image>
      </ImageView>
      <Button fx:id="chatButton" layoutX="92.0" layoutY="236.0" mnemonicParsing="false" onAction="#chatButtonPressed" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: ffffff;" text="Chat" textFill="#000000b2">
         <font>
            <Font name="Arial Narrow" size="24.0" />
         </font></Button>
      <Button fx:id="getHelpButton" layoutX="348.0" layoutY="234.0" mnemonicParsing="false" onAction="#getHelpButtonPressed" prefHeight="55.0" prefWidth="118.0" style="-fx-background-color: ffffff;" text="Find Others">
         <font>
            <Font name="Arial Narrow" size="18.0" />
         </font></Button>
      <ImageView fx:id="overchat" fitHeight="150.0" fitWidth="161.0" layoutX="67.0" layoutY="59.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@conversation.png" />
         </image>
      </ImageView>
      <ImageView fx:id="overfindothers" fitHeight="150.0" fitWidth="148.0" layoutX="333.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@promotion.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
