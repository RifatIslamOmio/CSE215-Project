            <Image url="@recruitment.png" />
         </image>
      </ImageView>
      <ImageView layoutX="78.0" layoutY="73.0">
         <image>
            <Image url="@chat.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="chatButton" layoutX="89.0" layoutY="256.0" onAction="#chatButtonPressed" prefHeight="43.0" prefWidth="107.0" style="-fx-background-color: #dad7d7; -fx-background-radius: 8;" text="Chat" textFill="#171717">
         <font>
            <Font name="Segoe UI Symbol" size="16.0" />
